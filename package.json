{
    "name": "@bigaru/lysa",
    "description": "A practical utility library for TypeScript.",
    "version": "0.0.4",
    "author": "bigaru",
    "type": "module",
    "main": "./dist/cjs/index.js",
    "browser": "./dist/browser/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/types/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/types/index.d.ts",
            "import": "./dist/esm/index.js",
            "default": "./dist/esm/index.js",
            "require": "./dist/cjs/index.js",
            "node": "./dist/cjs/index.js"
        }
    },
    "license": "Apache-2.0",
    "sideEffects": false,
    "files": [
        "dist",
        "README.md",
        "LICENSE.txt"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bigaru/lysa.git"
    },
    "bugs": {
        "url": "https://github.com/bigaru/lysa/issues"
    },
    "homepage": "https://github.com/bigaru/lysa#readme",
    "keywords": [
        "functional",
        "utils",
        "utilities",
        "toolkit",
        "lazy"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build:rest": "bunx tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json",
        "build:browser": "bun build --outdir ./dist/browser --sourcemap=linked ./src/index.ts",
        "build": "bun run build:rest && bun run build:browser",
        "test": "bun test"
    },
    "devDependencies": {
        "@types/bun": "^1.1.2",
        "lodash": "^4.17.21",
        "mitata": "^0.1.11",
        "prettier": "^3.2.5",
        "typescript": "^5.6.3"
    },
    "dependencies": {
        "rxjs": "^7.8.1"
    }
}
